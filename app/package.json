{
  "name": "crosshare-next",
  "version": "0.1",
  "description": "Crosshare ported to next.js",
  "author": "Mike D",
  "private": true,
  "type": "module",
  "browserslist": [
    ">0.2%",
    "last 2 versions",
    "unreleased versions",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "scripts": {
    "supportedBrowsers": "echo \"export const browserRegex = $(browserslist-useragent-regexp --allowHigherVersions);\" > lib/supportedBrowsers.ts",
    "lint": "npx next lint",
    "format": "npx prettier \"../**/*.ts(x)?\" --write",
    "checkFormat": "npx prettier \"../**/*.ts(x)?\" --check",
    "clean": "rimraf nextjs/",
    "dev": "GOOGLE_APPLICATION_CREDENTIALS=../serviceAccountKey.json next dev",
    "analyze": "ANALYZE=true next build",
    "build": "next build",
    "start": "next start",
    "extract": "npx lingui extract",
    "compileI18n": "npx lingui compile",
    "predeploy": "tsc && yarn clean && next build",
    "prodDeploy": "npx firebase deploy --only hosting:prod",
    "stagingDeploy": "npx firebase deploy --only hosting:staging",
    "test": "NODE_OPTIONS='--experimental-vm-modules' npx jest --watch"
  },
  "engines": {
    "node": "18"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@juggle/resize-observer": "^3.4.0",
    "@lingui/format-po-gettext": "^4.5.0",
    "@lingui/react": "^4.5.0",
    "@lingui/swc-plugin": "^4.0.4",
    "@popperjs/core": "^2.11.8",
    "@sentry/nextjs": "^7.70.0",
    "@use-it/event-listener": "^0.1.7",
    "canvas": "^2.11.2",
    "color2k": "^2.0.2",
    "date-fns": "^2.30.0",
    "fast-deep-equal": "^3.1.3",
    "feed": "^4.2.2",
    "firebase": "10.4.0",
    "firebase-admin": "^11.10.1",
    "fp-ts": "^2.16.1",
    "hast-util-to-html": "^9.0.0",
    "hast-util-truncate": "^2.0.0",
    "idb-keyval": "6.2.1",
    "io-ts": "^2.2.20",
    "jspdf": "^2.5.1",
    "levelup": "^5.1.1",
    "lodash": "^4.17.21",
    "mdast-util-find-and-replace": "^3.0.1",
    "next": "13.5.2",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-csv": "^2.2.2",
    "react-dom": "^18.2.0",
    "react-firebase-hooks": "5.1.1",
    "react-fluid-table": "^0.4.2",
    "react-icons": "^4.11.0",
    "react-image-crop": "^10.1.8",
    "react-popper": "^2.3.0",
    "react-window": "^1.8.9",
    "rehype-external-links": "^3.0.0",
    "rehype-react": "8.0.0",
    "rehype-stringify": "^10.0.0",
    "remark-directive": "^3.0.0",
    "remark-gfm": "^4.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.0.0",
    "rocksdb": "^5.2.1",
    "twemoji-parser": "^14.0.0",
    "unified": "^11.0.3",
    "unist-util-is": "^6.0.0",
    "unist-util-visit": "^5.0.0",
    "use-resize-observer": "^9.1.0"
  },
  "dependencyComments": {
    "prop-types": "Needed by react-csv and apparently not specified. Breaks sharedstats when missing"
  },
  "devDependencies": {
    "@emotion/eslint-plugin": "^11.11.0",
    "@emotion/jest": "^11.11.0",
    "@firebase/rules-unit-testing": "^3.0.1",
    "@lingui/cli": "^4.5.0",
    "@lingui/macro": "^4.5.0",
    "@modern-dev/jsbn": "^1.2.5",
    "@next/bundle-analyzer": "^13.5.2",
    "@playwright/test": "1.38.1",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@types/hast": "^3.0.1",
    "@types/jest": "^29.5.5",
    "@types/jest-in-case": "^1.0.6",
    "@types/levelup": "^5.1.3",
    "@types/lodash": "^4.14.199",
    "@types/node": "^20.6.3",
    "@types/react": "^18.2.22",
    "@types/react-csv": "^1.1.5",
    "@types/react-dom": "^18.2.7",
    "@types/react-window": "^1.8.5",
    "@types/rocksdb": "^3.0.1",
    "@types/twemoji-parser": "^13.1.2",
    "@types/unist": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "browserslist-useragent-regexp": "^4.1.0",
    "csv": "^6.3.3",
    "eslint": "8.50.0",
    "eslint-config-next": "13.5.2",
    "eslint-config-prettier": "^9.0.0",
    "firebase-tools": "^12.5.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-in-case": "^1.0.2",
    "mdast-util-from-markdown": "^2.0.0",
    "mdast-util-to-hast": "^13.0.2",
    "mockdate": "^3.0.5",
    "prettier": "^3.0.3",
    "prettier-2": "npm:prettier@^2",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "unist-util-remove-position": "^5.0.0",
    "wait-for-expect": "^3.0.2"
  }
}
